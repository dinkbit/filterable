{
    "name": "dinkbit/filterable",
    "description": "Make your eloquent models filterable with ease.",
    "license": "MIT",
    "keywords": [
        "filter",
        "eloquent",
        "model",
        "laravel"
    ],
    "authors": [
        {
            "name": "Joseph Cohen",
            "email": "joseph.cohen@dinkbit.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.0|~5.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Dinkbit\\Filterable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinkbit\\Filterable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
